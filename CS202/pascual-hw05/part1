Part 1: Be a disassembler
  Disassemble
    mov r5, #1066
    0000 0011 1010 0000 0101 0100 0010 1010
    0x03a0542a

    sub r9, r4, #19
    0000 0010 0100 0100 1001 0000 0001 0011
    0x02449013

    str r0, r5, r2, 9
    0000 0101 1000 0101 0000 0100 1000 0010

    ldr r3, r4
    0000 0101 1001 Rn Rt

  Assemble

    0x0356002a
      0000 0011 0101 0110 0000 0000 0010 1010
      compare (immediate)
      cmp r6, #42

    0x0082b004
      0000 0000 1000 0010 1011 0000 0000 0100
      add(register)
      add r11, r2, r4

    0x038004d2
      0000 0011 1000 0000 0000 0100 1101 0010
      bitwise or(immediate)
      orr r0, r0, 1234

    0x05934e88
      0000 0101 1001 0011 0100 1110 1000 1000
      load (base + immediate offset)
      ldr r4, [r3, #3720]
